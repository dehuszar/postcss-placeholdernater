{
  "name": "postcss-placeholdernater",
  "version": "0.0.1",
  "description": "PostCSS plugin to convert all selectors from a source file to a Sass or Stylus placeholder selector",
  "scripts": {
    "prepublish": "babel test/test.js --out-dir dist --ignore /test/",
    "test-unformatted": "babel-tape-runner \"dist/test/*.js\"",
    "test": "npm run test-unformatted | tap-spec"
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "Sass",
    "Stylus",
    "PostCSS",
    "postcss-plugin"
  ],
  "author": "Samuel Allen <dehuszar@gmail.com>",
  "license": "MIT",
  "repository": "dehuszar/postcss-placeholdernater",
  "bugs": {
    "url": "https://github.com/dehuszar/postcss-placeholdernater/issues"
  },
  "homepage": "https://github.com/dehuszar/postcss-placeholdernater",
  "dependencies": {
    "postcss": "^5.0.14",
    "postcss-extend": "^1.0.1"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-tape-runner": "^2.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  }
}
